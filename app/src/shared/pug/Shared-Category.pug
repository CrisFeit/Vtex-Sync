extends layouts/_base

//- Template variables
block vars
    - var $storeName = 'Perfumaria'
    - var $templateName = 'Category'

//- Page styles
block append styles
    link(rel='stylesheet', href=`/arquivos/${$storeName}-${$templateName}.css`, media='all')/

//- Main Content 
block content
    include includes/_line
    include includes/_shelf
    
//- Page scripts
block append scripts
    script( defer type='text/javascript', src=`/arquivos/${$storeName}-${$templateName}.js`)
